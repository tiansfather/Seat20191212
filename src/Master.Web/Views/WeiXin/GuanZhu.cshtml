@{
    Layout=null;
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta name="viewport" content="width=device-width, height=device-height, initial-scale=1, maximum-scale=1, user-scalable=no" />

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>赛多利斯（Sartorius）</title>
    <link href="/css/style.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="/css/aos.css" />
</head>
<body>
    <div class="contain">
        <div class="nav">
            <a class="title" href="#"></a>
            <div class="date"></div>
            <div class="clear"></div>
        </div>
        <div class="table">
            <div class="logo" aos="fade-down" aos-easing="ease" aos-delay="200"><img src="/images/logo.png" width="100%"></div>
            <div class="scare" aos="fade-up" aos-easing="ease" aos-delay="400"><img src="~/qrcode.jpg" width="100%" /></div>
            <p class="text" aos="fade-up" aos-easing="ease" aos-delay="600">请先扫码关注公众号</p>

        </div>

    </div>
    <script src="http://libs.baidu.com/jquery/2.1.1/jquery.min.js" type="text/javascript"></script>
    <script>window.jQuery || document.write('<script src="/js/jquery-2.1.1.min.js"><\/script>')</script>
    <script src="/js/highlight.min.js"></script>
    <script src="/css/aos.js"></script>
    <script>
        AOS.init({
            easing: 'ease-out-back',
            duration: 1000
        });
    </script>
    <script>
        hljs.initHighlightingOnLoad();

        $('.hero__scroll').on('click', function (e) {
            $('html, body').animate({
                scrollTop: $(window).height()
            }, 1200);
        });
    </script>
</body>

</html>
